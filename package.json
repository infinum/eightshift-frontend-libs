{
	"name": "@eightshift/frontend-libs",
	"version": "6.0.2",
	"description": "A collection of useful frontend utility modules. powered by Eightshift",
	"author": {
		"name": "Eightshift team",
		"email": "team@eightshift.com",
		"url": "https://eightshift.com/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/infinum/eightshift-frontend-libs.git"
	},
	"keywords": [
		"frontend",
		"scss",
		"mixin",
		"js",
		"utility",
		"module",
		"WordPress"
	],
	"bugs": {
		"url": "https://github.com/infinum/eightshift-frontend-libs/issues"
	},
	"scripts": {
		"lintStyle": "stylelint **/*.scss",
		"lintJs": "eslint blocks/ setup/ webpack/ scripts/ babel/ linters/ .storybook/",
		"lint": "npm run lintJs && npm run lintStyle",
		"storybookBuild": "build-storybook -c .storybook -o docs/storybook",
		"storybook": "start-storybook -c .storybook",
		"sassdocBuild": "node ./node_modules/sassdoc/bin/sassdoc styles/scss --dest docs/sassdocs && cp ./styles/sassdocs/main.css ./docs/sassdocs/assets/css/ && cp ./styles/sassdocs/fonts/* ./docs/sassdocs/assets/css/",
		"docsBuild": "rm -rf docs && npm run sassdocBuild && npm run storybookBuild",
		"docsDeploy": "npm run docsBuild && gh-pages -d docs",
		"test": "jest --maxWorkers=2",
		"test:unit": "jest --testPathPattern=tests/unit",
		"test:integration": "jest --testPathPattern=tests/integration"
	},
	"homepage": "https://github.com/infinum/eightshift-frontend-libs#readme",
	"license": "MIT",
	"dependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/eslint-plugin": "^7.17.7",
		"@infinumjs/eslint-config-react-js": "^3.0.0",
		"@wordpress/api-fetch": "^5.2.2",
		"@wordpress/dependency-extraction-webpack-plugin": "^3.4.1",
		"@wordpress/dom-ready": "^2.11.0",
		"@wordpress/icons": "^2.8.0",
		"autoprefixer": "^10.4.4",
		"babel-loader": "^8.2.4",
		"browser-sync": "^2.27.9",
		"browser-sync-webpack-plugin": "^2.3.0",
		"classnames": "^2.3.1",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.22.4",
		"crypto-browserify": "^3.12.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"eslint": "^8.11.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.5.0",
		"file-loader": "^6.2.0",
		"husky": "^7.0.4",
		"import-glob-loader": "^1.1.0",
		"media-blender": "^2.1.0",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.4.12",
		"postcss-loader": "^6.2.1",
		"postcss-scss": "^4.0.4",
		"promisify-child-process": "^4.1.1",
		"raw-loader": "^4.0.2",
		"rc-slider": "^9.7.5",
		"react-html-parser": "^2.0.2",
		"react-select": "^5.2.2",
		"react-sortable-hoc": "^2.0.0",
		"regenerator-runtime": "^0.13.9",
		"sass": "^1.49.9",
		"sass-loader": "^12.6.0",
		"storybook": "^6.4.19",
		"stream-browserify": "^3.0.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.6.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"terser-webpack-plugin": "^5.3.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"webpack-manifest-plugin": "^5.0.0",
		"webpack-merge": "^5.8.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@eightshift/storybook": "^1.4.0",
		"@jest/globals": "^27.5.1",
		"babel-jest": "^27.5.1",
		"chalk": "^5.0.1",
		"del": "^6.0.0",
		"gh-pages": "^3.2.3",
		"jest": "^27.5.1",
		"react-test-renderer": "^17.0.2",
		"replace-in-file": "^6.3.2",
		"sassdoc": "^2.7.3"
	},
	"sideEffects": false,
	"resolutions": {
		"webpack": "5.70.0"
	}
}
